class P{
    String s = "Parent"; //can also be static
}
class C extends P{
    String s = "Child"; //can also be static
}
class Main{
    public static void main(){
        P p = new P();
        System.out.println(p.s);
        C c = new C();
        System.out.println(c.s);
        P p1 = new C();
        System.out.println(p1.s);
    }
}
