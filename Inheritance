class P{
    public void m1(){
        System.out.println("Parent");
    }
}
class C extends P {
    public void m2() {
        System.out.println("Child");
    }
}
class Main{
    public static void main(String[] args){
        P p = new P();
        p.m1();
       // p.m2(); Compiler error
        C c = new C();
        c.m1();
        c.m2();
        P p1 = new C();
        p1.m1();
        //p1.m2(); Compiler error
    }
}
