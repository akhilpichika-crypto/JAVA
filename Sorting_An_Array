public class Main{
    public static void main(String args[]){
        int[] nums = {1,2,1,4,5};
        System.out.println(Solution.isSorted(nums));
    }
}
class Solution {
    public static boolean isSorted(int[] nums) {
        boolean flag = true;
        for (int i=0 ; i<nums.length-1; i++){
            if(nums[i] > nums[i+1]){
                flag = false;
                break;
            }
            else flag = true;
        }
        return flag;
    }
}
