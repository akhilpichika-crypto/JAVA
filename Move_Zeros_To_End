public class Main{
    public static void main(String[] args){
        Solution sol = new Solution();
        int[] nums = {0,0,0,1,3,-2};
        sol.moveZeroes(nums);
        for (int i=0; i<nums.length; i++){
            System.out.print(nums[i] + " ");
        }
    }
}
class Solution {
    public void moveZeroes(int[] nums) {
        int j = -1;
        for(int i=0; i<nums.length; i++){
            if(nums[i]==0){
                j = i;
                break;
            }
        }
        if (j==-1){
            return;
        }
        for (int i=j+1; i<nums.length;i++){
            if(nums[i] != 0){
                int temp = nums[i];
                nums[i] = nums[j];
                nums[j] = temp;
                j++;
            }
        }
    }
}
