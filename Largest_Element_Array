public class Main{
    public static void main(String[] args){
        Solution sol = new Solution();
        int[] nums = {-4,-3,0,1,-8};
        System.out.println(sol.largestElement(nums));
    }
}
class Solution {
    public int largestElement(int[] nums) {
        int max = nums[0];
        for (int i=1; i<nums.length;i++){
            if (nums[i] > max){
                max = nums[i];
            }
        }
        return max;
    }
}
